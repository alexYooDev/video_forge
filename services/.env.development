# Development environment configuration for microservices
NODE_ENV=development

# Server Configuration
SERVER_PORT=8000
SERVER_HOST=localhost

# SQS Configuration
SQS_QUEUE_NAME=video-forge-video-processing-queue
SQS_POLLING_INTERVAL=5000

# Processing Configuration
MAX_CONCURRENT_JOBS=2
FFMPEG_THREADS=2

# AWS Configuration
AWS_REGION=ap-southeast-2

# Local fallback configurations (used when AWS SSM/Secrets Manager unavailable)
PG_HOST=localhost
PG_PORT=5432
PG_DATABASE=video_forge_dev
PG_USERNAME=postgres
PG_PASSWORD=password

# Cache Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
CACHE_ENABLED=true

# S3 Configuration
S3_BUCKET_NAME=video-forge-storage-dev

# Sample video for testing
SAMPLE_VIDEO_URL=https://sample-videos.com/zip/10/mp4/SampleVideo_1280x720_1mb.mp4